type Query {
  events: [Event]
  event(id: ID): Event

  organisers: [Organiser]
  organiser(id: ID): Organiser
}

type Event {
  id: ID
  name: String
  organiser: Organiser
  description: String
  venue: String
  venue_location: String
  availability: Int
  date: String
  image: String
  price: Float
}

type Organiser {
  id: ID
  name: String
  logo: String
  description: String
  events: [Event]
}

type Mutation {
  deleteEvent(id: ID): Event
  deleteOrganiser(id: ID): Organiser
}